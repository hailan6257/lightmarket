<!DOCTYPE html>
<html lang="en">
<head>
  <title>Fetch using PSQL, Express and Node.js</title>
</head>
<body>
    <div class="table-data">
<h2>Display Data using Express, Node.js & PSQL</h2>
<% for(let product of products) { %>

  <div><%= product.id %></div>
  <div><%= product.name %></div>

<% } %>


            <form method="POST" action='/products/filter'>
              <div>minPrice</div>
              <input type="text" id="minPrice" name="minPrice"><br>
              <div>maxPrice</div>
              <input type="text" id="maxPrice" name="maxPrice">
              <button type="send" class="btn btn-outline-primary">Send</button>
            </form>

    </table>
    </div>
</body>
</html>
